{% extends 'site/group/page.html.twig' %}

{% block pageTitle %}History of changes of {{ group.title }} - {% endblock %} 



{% block htmlHeader %}
	<META NAME="ROBOTS" CONTENT="NOINDEX, NOFOLLOW">
{% endblock %}

{% block tabHistory %}currentTab{% endblock %}

{% block innerContent %}

	<ul class="history">
		{% for historyItem in historyItems %}
			{% if historyItem is eventhistory %}
				{% include 'site/common/eventHistoryItem.html.twig' with { 'historyItem': historyItem} %}
			{% elseif historyItem is grouphistory %}
				{% include 'site/common/groupHistoryItem.html.twig' with { 'historyItem': historyItem} %}
			{% endif %}
		{% endfor %}
	</ul>

{% endblock %}


