{% extends 'site/page.html.twig' %}

{% block pageTitle %}Visibility Admin - {% endblock %} 

{% block breadcrumbs %}
	{% if not config.isSingleSiteMode %}<li><a href="/">{{ currentSite.title }}</a></li>{% endif %}
	<li><a href="/admin">admin</a></li>
	<li class="current"><a href="/admin/visibility">visibility</a></li>
{% endblock %}

{% block content %}


	<form action="#" method="post" class="styled">

		{{ form_errors(form) }}

		{{ form_row(form.is_web_robots_allowed) }}
		{{ form_rest(form) }}

		<input type="submit"  value="Save" />	
	</form>

{% endblock %}
