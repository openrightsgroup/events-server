{% extends 'site/importurl/page.html.twig' %}


{% block pageTitle %}Disable {{ importurl.title }} - {% endblock %} 


{% block actions %}
{% endblock %}


{% block innerContent %}

	<p>Note disabling the import will delete all future events.</p>


	<form action="#" method="post" class="styled">
		<input type="hidden" name="CSFRToken" value="{{ getCSFRToken() }}">
		<input type="hidden" name="disable" value="yes">

		<input type="submit"  value="Disable" />	
	</form>


{% endblock %}


	