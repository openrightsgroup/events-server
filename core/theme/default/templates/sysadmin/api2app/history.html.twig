{% extends 'sysadmin/page.html.twig' %}


{% block pageTitle %} API2 App {{ api2Application.title }} History - {% endblock %}

{% block content %}

	<ul class="history">
		{% for historyItem in historyItems %}
			{% if historyItem is eventhistory %}
				{% include 'sysadmin/common/eventHistoryItem.html.twig' %}
			{% elseif historyItem is grouphistory %}
				{% include 'sysadmin/common/groupHistoryItem.html.twig' %}
			{% elseif historyItem is venuehistory %}
				{% include 'sysadmin/common/venueHistoryItem.html.twig' %}
			{% elseif historyItem is areahistory %}
				{% include 'sysadmin/common/areaHistoryItem.html.twig' %}
			{% endif %}
		{% endfor %}
	</ul>

{% endblock %}
