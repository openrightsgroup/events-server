{% extends 'sysadmin/page.html.twig' %}

{% block content %}


	<table class="data">
		<tr>
			<th>Sent At</th>
		</tr>
		{% for email in emails %}
			<tr {% if loop.index is even %}class="alternative"{% endif %} >
				<td>{{ email.sentAt  |tolocaltimeformatted('D H:i jS M Y', currentTimeZone ) }}</td>
			</tr>
		{% endfor %}
	</table>

{% endblock %}
